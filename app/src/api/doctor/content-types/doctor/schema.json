{
  "kind": "collectionType",
  "collectionName": "doctors",
  "info": {
    "singularName": "doctor",
    "pluralName": "doctors",
    "displayName": "Doctor",
    "description": "Dental professionals"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "bio": {
      "type": "richtext",
      "required": true
    },
    "specialties": {
      "type": "json"
    },
    "education": {
      "type": "richtext"
    },
    "certifications": {
      "type": "richtext"
    },
    "personalInfo": {
      "type": "richtext"
    },
    "locations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::location.location",
      "mappedBy": "doctors"
    },
    "services": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::service.service"
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "metaTitle": {
      "type": "string",
      "maxLength": 60
    },
    "metaDescription": {
      "type": "text",
      "maxLength": 160
    }
  }
}

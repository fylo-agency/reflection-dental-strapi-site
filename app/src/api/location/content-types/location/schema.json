{
  "kind": "collectionType",
  "collectionName": "locations",
  "info": {
    "singularName": "location",
    "pluralName": "locations",
    "displayName": "Location",
    "description": "Dental office locations"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "address": {
      "type": "component",
      "repeatable": false,
      "component": "location.address",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email"
    },
    "hours": {
      "type": "component",
      "repeatable": true,
      "component": "location.business-hours"
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images"
      ]
    },
    "mapUrl": {
      "type": "string"
    },
    "latitude": {
      "type": "decimal"
    },
    "longitude": {
      "type": "decimal"
    },
    "services": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::service.service",
      "inversedBy": "locations"
    },
    "doctors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::doctor.doctor",
      "inversedBy": "locations"
    },
    "metaTitle": {
      "type": "string",
      "maxLength": 60
    },
    "metaDescription": {
      "type": "text",
      "maxLength": 160
    },
    "isPrimary": {
      "type": "boolean",
      "default": false
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
